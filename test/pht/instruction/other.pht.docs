(category "other"
    (instr
        (long   "debug")
        (short  "dbg")
        (symbol "_")
        (desc "Отладочная инструкция, игнорирует аргументы.")
        (usage
            "(debug arg0 argN)"
            [["arg*"] "аргументы."])
        (example
            """
            (use-ctx pht
                (app-fn
                    (debug)))
            """
            "Код работает на всех платформах!"))
    (instr
        (long "comment")
        (desc "Комментарий, игнорируется на уровне парсинга.")
        (usage
            "(comment arg0 argN)"
            [["arg*"] "аргументы."])
        (example
            """
            (use-ctx pht
                (comment "Выводим базу.")
                (app-fn
                    (println "Славься Сербский Брат!")))
            """
            "Код работает на всех платформах!")))