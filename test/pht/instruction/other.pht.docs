(category "other"
    (instr
        (long "comment")
        (desc "Комментарий, игнорируется на уровне парсинга.")
        (usage
            "(comment arg0 argN)"
            [["arg*"] "аргументы."])
        (example
            """
            (use-ctx pht
                (comment "Выводим базу.")
